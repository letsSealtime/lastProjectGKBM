<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
	PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

	<!-- tag(또는 node)의 순서에 주의 -->

	<!-- 전달인자로 null 줄 수 있음 -->
	<settings>
		<setting name="jdbcTypeForNull" value="NULL" />
	</settings>

	<!-- mapper에 사용할 타입이 길어서 짧게 별칭 지정 -->
	<typeAliases>
		<!--  상품정보관리꺼  -->
<typeAlias type="kr.or.GKBM.sku.P_skuDTO" alias="skuDTO"/>
<!--  원자재입고관리꺼  -->
<typeAlias type="kr.or.GKBM.material_in_out.P_material_in_outDTO" alias="mioDTO"/>
<!--  원자재출고관리꺼  -->
<typeAlias type="kr.or.GKBM.material_in_out2.P_material_in_outDTO2" alias="mioDTO2"/>
<!--  원자재현황꺼  -->
<typeAlias type="kr.or.GKBM.material_in_out3.P_material_in_outDTO3" alias="mioDTO3"/>
<!--  완제품입고관리꺼  -->
<typeAlias type="kr.or.GKBM.product_in_out.P_product_in_outDTO" alias="pioDTO"/>
<!--  완제품입고관리꺼  -->
<typeAlias type="kr.or.GKBM.product_in_out2.P_product_in_outDTO2" alias="pioDTO2"/>
<!--  작업표준서꺼  -->
<typeAlias type="kr.or.GKBM.work_method.P_work_methodDTO" alias="wmDTO"/>
<!--  bom꺼  -->
<typeAlias type="kr.or.GKBM.bom.P_bomDTO" alias="bDTO"/>
		<!-- 게시판 -->
		<typeAlias type="kr.or.GKBM.board.BoardDTO" alias="boardDTO" />
		<typeAlias type="kr.or.GKBM.board.BoardFileDTO" alias="boardFileDTO" />
		<typeAlias type="kr.or.GKBM.board.BoardCommentDTO" alias="commentDTO" />
		
	</typeAliases>



</configuration>